<!DOCTYPE html>
<html>
  <head>
    <title>Cube</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="page">
      <div class="page-main">
        <%= render 'layouts/header' %>
        <%= render 'layouts/navbar' %>

        <div class="my-3 my-md-5">
          <div class="container">
            <%= yield %>
          </div>
        </div>

      </div>
    </div>
  </body>
</html>
